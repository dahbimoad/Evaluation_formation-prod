{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "catalog": {
        "ClusterId": "catalog-cluster",
        "Match": {
          "Path": "api/Formation/{**catch-all}"
        }
      },
      "questionnaire": {
        "ClusterId": "questionnaire-cluster",
        "Match": {
          "Path": "/questionnaire/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "catalog-cluster": {
        "Destinations": {
          "catalog": {
            "Address": "http://localhost:5161/" 
          }
        }
      },
      "questionnaire-cluster": {
        "Destinations": {
          "questionnaire": {
            "Address": "http://localhost:5138/" 
          }
        }
      }
    }
  }
}
